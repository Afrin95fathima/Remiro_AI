{
  "name": "remiro-ai",
  "version": "1.0.0",
  "description": "Remiro AI - Your 12D Career Counsellor with Multi-Agent System",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
